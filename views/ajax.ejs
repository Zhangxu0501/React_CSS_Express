<script>
    $(function () {
        $("#test-b1").click(function () {
            $.get("/lx/lx",function (data){
                $("#test-p1").text("姓名："+data.name+"年龄"+data.age);
            })
        });

        $("#test-submit").click(function(){
            var data=$("#test-form").serialize();
            $.post("/lx/95",data,function(data){
                if(data.success){
                    alert("success!");
                }else{
                    alert("resubmit!");
                }
            });
        });

    });


</script>

